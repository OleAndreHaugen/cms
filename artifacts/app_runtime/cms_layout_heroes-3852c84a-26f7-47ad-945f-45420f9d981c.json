				{
					"fieldNo": "2a8f2b72-451a-4875-81ba-32422b99ecf7",
					"fieldName": "MetaData",
					"fieldParent": 99999,
					"fieldType": "neptune.Script",
					"script": [
						"==OBJECT STRING==",
						"const textColor = [{ key: \"\" }, { key: \"Black50\" }, { key: \"Body\" }, { key: \"Danger\" }, { key: \"Dark\" }, { key: \"Info\" }, { key: \"Light\" }, { key: \"Muted\" }, { key: \"Primary\" }, { key: \"Secondary\" }, { key: \"Success\" }, { key: \"Warning\" }, { key: \"White\" }, { key: \"White50\" }]\r",
						"const backgroundColor = [{ key: \"\" }, { key: \"Body\" }, { key: \"Danger\" }, { key: \"Dark\" }, { key: \"Info\" }, { key: \"Light\" }, { key: \"Primary\" }, { key: \"Secondary\" }, { key: \"Success\" }, { key: \"Transparent\" }, { key: \"Warning\" }, { key: \"White\" }]\r",
						"\r",
						"const metadata = {\r",
						"\r",
						"    enabledTitle: { type: \"CheckBox\", label: \"Show Fields\", text: \"Show Title\", default: true },\r",
						"    enabledSubTitle: { type: \"CheckBox\", label: \"\", text: \"Show SubTitle\", default: true },\r",
						"    enabledDescription: { type: \"CheckBox\", label: \"\", text: \"Show Description\", default: true },\r",
						"\r",
						"    title: { type: \"Input\", label: \"Title\", default: \"Centered Hero\", visible: false },\r",
						"    subTitle: { type: \"Input\", label: \"SubTitle\", default: \"Sub Title\", visible: false },\r",
						"    description: { type: \"Input\", label: \"Description\", default: \"Description\", visible: false },\r",
						"\r",
						"    // sectionProperties: { type: 'Title', label: 'Properties' },\r",
						"\r",
						"\r",
						"    // sectionContainer: { type: 'Title', label: 'Colors' },\r",
						"    backgroundImage: { type: \"Input\", label: \"Background Image\", default: \"\" },\r",
						"    backgroundColor: { type: \"SingleSelect\", label: \"Background Color\", default: \"\", items: backgroundColor },\r",
						"    backgroundColorHex: { type: \"Color\", label: \"Background Color HEX\", default: \"\" },\r",
						"    height: { type: \"Input\", label: \"Container height\", default: \"\" },\r",
						"\r",
						"    imageTop: { type: \"Image\", label: \"Image Top\", default: \"\" },\r",
						"    imageBottom: { type: \"Image\", label: \"Image Bottom\", default: \"\" },\r",
						"\r",
						"    titleColor: { type: \"SingleSelect\", label: \"Title Color\", default: \"\", items: textColor },\r",
						"    subTitleColor: { type: \"SingleSelect\", label: \"SubTitle Color\", default: \"\", items: textColor },\r",
						"    descriptionColor: { type: \"SingleSelect\", label: \"Description Color\", default: \"\", items: textColor },\r",
						"\r",
						"}\r",
						"\r",
						""
					],
					"customComponent": false,
					"request": [],
					"response": [],
					"attributes": [],
					"data": []
				},
				{
					"fieldNo": "402e2612-1a79-4e72-eff2-9165b7192db5",
					"fieldName": "Report",
					"fieldParent": 99999,
					"fieldType": "neptune.Script",
					"script": [
						"==OBJECT STRING==",
						"const report = {\r",
						"\r",
						"    metadata: metadata,\r",
						"\r",
						"    start: function () {\r",
						"        sap.n.cmsRuntime.initApp(this);\r",
						"    },\r",
						"\r",
						"    init: function (config, runtime) {\r",
						"\r",
						"        if (!config) config = {};\r",
						"\r",
						"        console.log(\"Init \", config)\r",
						"\r",
						"        modelAppConfig.setData(config);\r",
						"        modelAppConfig.refresh();\r",
						"\r",
						"        masterContainer.onAfterRendering = function () {\r",
						"            if (config.backgroundImage) {\r",
						"                const elem = masterContainer.getDomRef();\r",
						"                if (elem) elem.style.background = \"url(\" + config.backgroundImage + \")\";\r",
						"            }\r",
						"        };\r",
						"\r",
						"\r",
						"    }\r",
						"\r",
						"}\r",
						"\r",
						"\r",
						"if (sap.n && sap.n.cmsRuntime) {\r",
						"    report.start();\r",
						"} else {\r",
						"    sap.n.Shell.attachBeforeDisplay(function (config) {\r",
						"        report.init(config);\r",
						"    });\r",
						"}\r",
						""
					],
					"customComponent": false,
					"request": [],
					"response": [],
					"attributes": [],
					"data": []
				}
			]
		},
